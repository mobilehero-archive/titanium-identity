{
	"name": "@titanium/identity",
	"version": "2.0.7",
	"description": "Axway Amplify modules to use Fingerprint authentication, Keychain Access and Face authentication (iOS) with Appcelerator Titanium.",
	"main": "index.js",
	"scripts": {
		"list": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"postinstall": "npx @titanium/tiapp-xml install",
		"postuninstall": "npx @titanium/tiapp-xml uninstall",
		"prepublishOnly": "git pull && bump insane",
		"test": "npm install --ignore-scripts && npm run list"
	},
	"author": "Axway",
	"repository": {
		"type": "git",
		"url": "github:brentonhouse/titanium-identity"
	},
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/brentonhouse/titanium-identity/issues"
	},
	"homepage": "https://brenton.house/saying-goodbye-to-axway-amplify-titanium-31a44f3671de",
	"keywords": [
		"titanium",
		"mobile",
		"alloy",
		"turbo",
		"axway",
		"appcelerator",
		"ios",
		"android",
		"iphone",
		"ipad",
		"sdk",
		"plugin",
		"amplify",
		"api-builder",
		"appcelerator-titanium",
		"titanium-sdk",
		"titanium-mobile",
		"titanium-module",
		"axway-sdk",
		"axway-amplify",
		"brenton-house"
	],
	"titanium": {
		"type": "native-module",
		"platform": {
			"ios": {
				"moduleid": "ti.identity",
				"name": "titanium-identity",
				"version": "3.0.0",
				"apiversion": 2,
				"architectures": [
					"armv7",
					"arm64",
					"i386",
					"x86_64"
				],
				"minsdk": "9.2.0",
				"guid": "ae6ffc93-6e6e-4251-8373-b0cb263a1662"
			},
			"android": {
				"moduleid": "ti.identity",
				"name": "titanium-identity",
				"version": "3.0.3",
				"apiversion": 4,
				"architectures": [
					"arm64-v8a",
					"armeabi-v7a",
					"x86",
					"x86_64"
				],
				"minsdk": "9.0.0",
				"guid": "c3d987a8-8bd4-42cd-a3e4-2a75952d1ea0"
			}
		}
	},
	"devDependencies": {
		"@titanium/eslint-config": "1.0.1",
		"eslint": "^7.18.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsdoc": "^31.0.8",
		"eslint-plugin-jsonc": "^0.8.2",
		"eslint-plugin-promise": "^4.2.1"
	}
}
