{
	"name": "@titanium/identity",
	"version": "1.0.29",
	"description": "Native modules to add Fingerprint/FaceId/keychain access to Titanium native mobile apps",
	"main": "index.js",
	"scripts": {
		"list-files": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"postinstall": "tiapp-install-module",
		"postuninstall": "tiapp-uninstall-module",
		"prep": "npm install --ignore-scripts && npm run list-files",
		"prepublishOnly": "git pull && bump",
		"test": "echo \"no test specified\" && exit 0"
	},
	"author": "Axway",
	"repository": {
		"type": "git",
		"url": "github:brentonhouse/titanium-identity"
	},
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/brentonhouse/titanium-identity/issues"
	},
	"homepage": "https://github.com/brentonhouse/titanium-identity#readme",
	"keywords": [
		"titanium",
		"mobile",
		"alloy",
		"turbo",
		"axway",
		"appcelerator",
		"ios",
		"android",
		"iphone",
		"ipad",
		"titanium-native-module",
		"brenton-house"
	],
	"titanium": {
		"type": "native-module",
		"platform": {
			"ios": {
				"moduleid": "ti.identity",
				"name": "ti.identity",
				"version": "1.0.6",
				"apiversion": 2,
				"architectures": [
					"armv7",
					"arm64",
					"i386",
					"x86_64"
				],
				"minsdk": "6.2.0",
				"guid": "ae6ffc93-6e6e-4251-8373-b0cb263a1662"
			},
			"android": {
				"moduleid": "ti.identity",
				"name": "titanium-identity",
				"version": "2.1.1",
				"apiversion": 4,
				"architectures": [
					"arm64-v8a",
					"armeabi-v7a",
					"x86"
				],
				"minsdk": "7.0.0",
				"guid": "c3d987a8-8bd4-42cd-a3e4-2a75952d1ea0"
			}
		}
	},
	"devDependencies": {
		"@titanium/eslint-config": "0.0.11",
		"eslint": "^7.6.0"
	},
	"dependencies": {
		"@titanium/tiapp-xml": "^0.9.5"
	}
}
